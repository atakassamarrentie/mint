{
  "name": "UserExt",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Admin": true
  },
  "properties": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "picture": {
      "type": "string",
      "required": false
    },
    "employee": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "depart": {
      "type": "string",
      "required": false,
      "default": false
    },
    "occupation": {
      "type": "string",
      "required": false,
      "default": false
    },
    "phone": {
      "type" : "strong",
      "required": false,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [],
  "methods": {}
}
