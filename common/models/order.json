{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Logging": true,
    "Admin": true
  },
  "properties": {
    "order_date": {
      "type": "date",
      "required": true
    },
    "product_name": {
      "type": "string",
      "required": true
    },
    "product_category": {
      "type": "string",
      "required": true
    },
    "expected_date": {
      "type": "date",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true
    },
    "total": {
      "type": "number",
      "required": true
    },
    "quantity": {
      "type": "number",
      "required": true
    },
    "completed": {
      "type": "boolean",
      "default": false
    },
    "username": {
      "type": "string"
    },
    "partnerName": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "partner": {
      "type": "belongsTo",
      "model": "Partners",
      "foreignKey": ""
    },
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": ""
    },
    "payment": {
      "type": "belongsTo",
      "model": "Payment",
      "foreignKey": ""
    },
    "user": {
      "type": "belongsTo",
      "model": "UserExt",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "order_read",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "order_write",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "order_write",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
